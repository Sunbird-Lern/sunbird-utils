{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "CompositeIdentityObject",
  "description": "This extension provides a reference to certificate",
  "type": "object",
  "definitions": {
    "ISODate": {
      "description": "ISO 8601 date format string YYYY-MM-DD or YYYYMMDD",
      "type": "string",
      "pattern": "^([0-9]{4})(-?)(1[0-2]|0[1-9])\\2(3[01]|0[1-9]|[12][0-9])$"
    }
  },
  "properties": {
    "type": {
      "type": "array"
    },
    "components": {
      "type": "object"
    },
    "name": {
      "type": "string"
    },
    "photo": {
      "type": "string"
    },
    "dob": {
      "type": "string"
    },
    "gender": {
      "enum": [
        "OTHER",
        "MALE",
        "FEMALE"
      ]
    },
    "tag": {
      "type": "string"
    },
    "urn": {
      "type": "string"
    },
    "url": {
      "type": "string"
    }
  }
}